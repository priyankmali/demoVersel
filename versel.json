{
  "version": 2,
  "builds": [
    {
      "src": "project/wsgi.py",
      "use": "@vercel/python"
    },
    {
        "src" : "build.sh",
        "use" : "@vercel/static-build",
        "config" : { "distDir" : "staticfiles" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "project/wsgi.py"
    },
    {
        "src" : "/static/(.*)",
        "dest" : "/static/$1"
    }
  ]
}
